{
  "name": "seq-example",
  "description": "example of failing sequence replication",
  "version": "0.0.0",
  "repository": {
    "url": "nrw/seq-example"
  },
  "main": "server.js",
  "scripts": {
    "start": "npm build; node server.js",
    "test": "tap test/*.coffee",
    "watch": "supervisor -w . -x tap -e 'coffee|js' -n exit -q -- --tap test/*.coffee | faucet",
    "dev": "supervisor -e 'coffee|html' -q -n exit -x browserify -- client.coffee -t coffeeify -o bundle.js",
    "build": "browserify client.js -o bundle.js"
  },
  "dependencies": {
    "coffee-script": "1.x.x",
    "tape": "2.x.x",
    "crdt": "^3.6.0",
    "level-scuttlebutt": "^5.0.6",
    "level": "^0.18.0",
    "ecstatic": "^0.4.13",
    "shoe": "0.0.15",
    "through": "^2.3.4",
    "level-sublevel": "^5.2.0",
    "udid": "^0.1.4",
    "coffeeify": "^0.6.0",
    "domready": "^1.0.4",
    "reconnect-core": "0.0.1",
    "reconnect-widget": "^1.0.1",
    "level-js": "^1.1.2",
    "levelup": "^0.18.2"
  },
  "devDependencies": {
    "tap": "0.x.x",
    "faucet": "0.x.x"
  }
}
